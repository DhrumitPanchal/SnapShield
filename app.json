{
  "expo": {
    "name": "SnapShield",
    "slug": "SnapShield",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/logo.jpeg",
    "scheme": "com.dhrumit_2.SnapShield",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.jpeg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dhrumit_2.SnapShield"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.jpeg"
    },
    "plugins": [
      "expo-router",
      "@react-native-google-signin/google-signin",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/images/logo.jpeg",
          "dark": {
            "image": "./assets/images/dark_logo.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2d76ce7c-dbdc-4901-8f76-28c053a1cbc6"
      }
    }
  }
}